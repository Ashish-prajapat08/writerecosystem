{
  "routes": [
    {
      "src": "/auth/callback",
      "dest": "/index.html"
    },
    {
      "src": "/feed(/?)(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/reset-password",
      "dest": "/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}